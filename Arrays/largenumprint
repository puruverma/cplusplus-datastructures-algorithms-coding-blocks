#include<iostream>
#include<algorithm>
using namespace std;
bool compare(int a,int b)
	{
		return a>b;
	}
int main() {
	int t,m,a[1000000];
	cin>>t;
	cin>>m;
	while(t>0){
		for(int i=0;i<m;i++){
			cin>>a[i];
		}
		sort(a,a+m,compare);
		for(int i=0;i<m;i++){
			cout<<a[i];
		}
		t--;
	}
	return 0;
}
